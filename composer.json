{
    "name": "bennent/geauth",
    "description": "Laravel RBAC permission package base on GE-auth backend with LayUI",
    "keywords": [
        "laravel",
        "auth",
        "rbac",
        "permissions",
        "php",
        "roles"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "laravel/framework": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Bennent\\Geauth\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bennent\\Geauth\\GeauthServiceProvider"
            ],
            "aliases": {
                "geauth": "Bennent\\Geauth\\GeauthFacade"
            }
        }
    }
}
